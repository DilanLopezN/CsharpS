            <td class="dojoTextBoxIECorrect">
                <table class="Table" style="width: 100%">
                    <tr>
                        <td style="width: 50%">
                            <input data-dojo-type="dijit/form/FilteringSelect" style="width: 70px;" id="nm_frequencia" name="nm_frequencia" required="required"/> Dia(s)
                        </td>

                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td style="padding-bottom: 10px; padding-top: 10px;"><label for="name">Data de Término*:</label></td>
            <td class="dojoTextBoxIECorrect">
                <table class="Table" style="width: 100%">
                    <tr>
                        <td style="width: 50%">
                            <input type="radio" data-dojo-type="dijit/form/RadioButton" name="id_data_termino" id="id_data_termino_1" value="1"/>
