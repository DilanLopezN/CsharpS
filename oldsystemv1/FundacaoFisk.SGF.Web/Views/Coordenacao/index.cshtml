<div id="cadSala" data-dojo-type="dijit.Dialog" title="Cadastro de Sala" class="Dialogo">

    <div class="sessao_form" style="margin-left: -10px;">
        <span id="counter_form">Sua sess&atilde;o expira em 59 min 59 seg</span>
    </div>

    <div class="corpo_interno">
        <form data-dojo-type="dojox/form/Manager" id="formSala" data-dojo-id="formSala" enctype="multipart/form-data" class="Formulario">
            <div class="apresentadorMsg" style="display: none;" id="apresentadorMensagemSala"></div>
            <input id="cd_sala" name="cd_sala" type="hidden" value="0" />
