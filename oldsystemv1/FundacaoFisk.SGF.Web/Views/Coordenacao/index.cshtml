                <input name="id_sala_ativa" type="checkbox" id="id_sala_ativa" checked="checked" data-dojo-type="dijit/form/CheckBox" />
                <label for="nm_vaga_sala">N° Vaga:</label>
                <input data-dojo-type="dijit/form/NumberSpinner" id="nm_vaga_sala" name="nm_vaga_sala" value="0" data-dojo-props="smallDelta:1, constraints:{min:0,max:60,places:0,pattern:'00'}" onkeypress="mascaraFloat(document.getElementById('nm_vaga_sala'));" required="true" />
                <div style="padding-top: 10px">
