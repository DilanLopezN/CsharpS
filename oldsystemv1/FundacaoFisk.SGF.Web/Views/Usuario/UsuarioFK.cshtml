 <input id="idOrigemUsuarioFK" type="hidden" />
<div class="corpo_interno">
    <div class="apresentadorMsg" id="apresentadorMensagemProUsuario" style="display:none;"></div>
    <div class="EspacoCaixa caixaPesquisa">
        <table class="Table">
            <tr class="Pesquisa">
                    <td class=" AlinhaDireita" style="width: 5%;">
                        <label for="pesquisatextFK">Usuário: </label>
                    </td>
                    <td style="width: 29%;" class="dojoTextBoxIECorrect">
                        <input id="pesquisatextFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox" class="campoGrande" style="width: 100%">
                    </td>
                    <td class="AlinhaDireita" style="width: 5%;">
                        <label for="nomPessoaFK">Nome: </label>
                    </td>
                    <td style="width: 29%;" class=" dojoTextBoxIECorrect">
                        <input id="nomPessoaFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox" style="width: 100%">
                    </td>
                    <td class="AlinhaDireita" style="width: 7%;">
                        <label class="AlinhaDireita;" for="inicioUsuarioFK">Início:</label>
                    </td>
                    <td style="width: 2%;">
                        <input type="checkbox" id="inicioUsuarioFK" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
                    </td>

                    @*<td style="width: 5%;" class="tdRight">*@
                    <td class="tdRight" style="width: 10%" rowspan="2">
                        <input data-dojo-type="dijit/form/Button" data-dojo-props="iconClass:'dijitEditorIconSearchSGF'" type="button" id="pesquisarUsuarioFK" />
                    </td>
                </tr>
        </table>
        
    </div>
    <div style="padding-top: 5px; width: 100%">
        <div class="linha_acoes" style="width: 100%"></div>
        <table style="width: 100%">
            <tr style="width: 100%">
                <td style="text-align: center; width: 100%;">
                    <input type="button" id="selecionaUsuarioFK" title="Selecionar" />
                    <input type="button" id="fecharUsuarioFK" title="Fechar" />

                </td>
            </tr>
        </table>
        <div class="linha_acoes" style="width: 100%"></div>
    </div>
    <input id="ehSelectGradeAlunoFK" type="hidden" value="false" />
    <div style="width: 100%; height: 415px;">
        <div data-dojo-type="dijit/layout/ContentPane" id="gridUsuarioFK" style="width: 100%; height: 410px; padding-top: 5px; padding-left: 0px">
            <div id="gridPesquisaUsuarioFK"></div>
        </div>
    </div>
</div>