@{
    ViewBag.Title = "Funcionário/Professor";
    Layout = "~/Views/Shared/_layout.cshtml";
}

@section head {
    <link rel="stylesheet" href="~/Content/themes/base/dojo1.8.5r/dojox/calendar/themes/claro/Calendar.css">
    <link href="~/Content/themes/base/dojo1.8.5r/dojox/form/resources/CheckedMultiSelect.css" rel="stylesheet" />
}

@section scripts {
    @Scripts.Render("~/bundles/funcionario")
}

@section scriptsBody{
    <script type="text/javascript">
        document.getElementById("labelTitulos").innerHTML = "Funcionário/Professor";
        //**** Requerires para a Tabulação ****\\
        require([
                "dojo/NodeList-traverse",
                "dojo/_base/event",
                "dojox/validate/web",
                "dojox/validate/check",
                "dijit/form/NumberTextBox",
                "dijit/form/ValidationTextBox",
                "dijit/form/Form",
                "dojox/form/Manager",
                "dijit/form/TimeTextBox",
                "dojox/charting/Chart",
                "dojox/charting/plot2d/Columns",
                "dojox/charting/axis2d/Default",
                "dojox/charting/widget/Legend",
                "dojox/charting/themes/MiamiNice"
        ]);
        montaGridFuncionario('@Permissoes');
    </script>
}
<input type="hidden" id="link" value="" />
<div class="apresentadorMsg" id="apresentadorMensagem" style="display: none;"></div>
<script type="text/javascript">
    showCarregando();
</script>

@*Criação da Pesquisa*@
<div class="EspacoCaixa caixaPesquisa">
    @* inputs sinalizadores *@
    <input type="hidden" id="setValuePermissoes" value="" />
    <input id="isPesquisa" value="false" type="hidden" />

    <table class="Table">
        <tr>
            <td class="tdRight" style="width: 5%;">
                <label for="nome">Nome:</label>
            </td>
            <td style="width: 25%;" class="dojoTextBoxIECorrect">
                <input type="text" id="_nome" name="Nome" style="width: 100%;" data-dojo-type="dijit/form/TextBox" />
            </td>
            <td class="tdRight" style="width: 9%;">
                <label for="fantasia">Nome Reduzido:</label>
            </td>
            <td class="dojoTextBoxIECorrect" style="width: 24%;">
                <input type="text" id="_reduzido" name="_reduzido" style="width: 100%;" data-dojo-type="dijit/form/TextBox" />
            </td>
            <td class="tdRight" style="width: 3%">
                <label class="AlinhaDireita;" for="inicioCurso">Início:</label>
            </td>
            <td style="width: 2%">
                <input type="checkbox" id="inicioFunc" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
            </td>
            <td class="tdRight" style="width: 2%">
                <label for="statusFunc" class="AlinhaDireita;">Status:</label>
            </td>
            <td style="width: 23%;" class="dojoTextBoxIECorrect" colspan="3">
                <table>
                    <tr>
                        <td  class="dojoTextBoxIECorrect" style="width: 40%">
                            <input id="statusFunc"  style="width:100%;" />
                        </td>
                        <td class="tdRight" style="width: 15%">
                            <label class="AlinhaDireita;" for="nm_sexo">Gênero:</label>
                        </td>
                        <td style="width: 45%;" class="dojoTextBoxIECorrect">
                            <select id="nm_sexo" name="nm_sexo" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;"></select>
                        </td>
                    </tr>
                </table>
            </td>
            <td class="tdRight" style="width: 7%;" rowspan="2">
                <input id="pesquisarFunc" title="Pesquisar" />
            </td>
        </tr>
        <tr>
            <td class="tdRight" style="width: 5%">
                 <label>CPF:</label>
            </td>
            <td style="width: 25%;" class="dojoTextBoxIECorrect">
                 <input type="text" id="_cpf" name="CnpjCpf" size="11" style="width: 55%;" data-dojo-type="dijit/form/TextBox" />
            </td>
            <td class="tdRight" style="width: 9%;">
               <label class="AlinhaDireita;" for="ckProfessor">Tipo:</label>
            </td>
            <td class="dojoTextBoxIECorrect" style="width: 29%;" colspan="3">
                <table class="Table">
                    <tr>
                        <td style="width: 35%;" class="dojoTextBoxIECorrect">
                             <select id="pesqFuncTipo" name="pesqFuncTipo" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;"></select>
                        </td>
                        <td class="tdRight" style="width: 35%;display:none;" id="lblCkCoordenador">
                            <label class="AlinhaDireita;" for="ckCoordenador">Coordenador:</label>
                        </td>
                        <td style="width: 30%; display:none;" class="tdRight" id="tdCkCoordenador">
                            <select id="coordenadorPesq" name="coordenadorPesq" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;" ></select>
                        </td>
                         <td class="tdRight" style="width: 35%;display:none;" id="lblCkColaboradorCyber">
                            <label class="AlinhaDireita;" for="ckColaboradorCyber">Colaborador Cyber:</label>
                        </td>
                        <td style="width: 30%;display:none;" class="tdRight" id="tdCkColaboradorCyber">
                             <select id="colaboradorCyberPesq" name="colaboradorCyberPesq" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;" ></select>
                        </td>
                        <td colspan="2" style="width:50%" id="tdCampoVazio"></td>
                    </tr>
                </table>
            </td>
            <td class="tdRight" style="width: 2%">
                <label>Cargo:</label>
            </td>
            <td style="width: 23%;" class="dojoTextBoxIECorrect nowrap">
                <input type="hidden" name="pesCdAtividade" id="pesCdAtividade" />
                <table class="Table">
                    <tr>
                        <td style="width: 80%;" >
                           <input type="text" name="atividadeFunc" style="width: 100%;" id="atividadeFunc" data-dojo-type="dijit/form/TextBox"  disabled="disabled"/>
                        </td>
                         <td class="tdRight" style="width: 18%">
                            <input id="pesFkCargo" style="min-width: 18px" />
                             <input id="limparFkCargoPes" style="min-width: 18px" />     
                        </td>
                    </tr>
                </table>            
            </td>
        </tr>
    </table>
</div>
@*Criação barra de ação*@
