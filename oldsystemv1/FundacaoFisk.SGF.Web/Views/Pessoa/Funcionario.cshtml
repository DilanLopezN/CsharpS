                                    <input id="proFkCargo" style="min-width: 18px" />
                                    <input id="limparFkCargo" style="min-width: 18px" />
                                </td>
                            </tr>
                            <tr>
                                <td class="tdRight" style="width: 23%;">
                                    <label for="idColaboradorCyber">Colaborador Cyber:</label>
                                </td>
                                <td style="width: 20%;">
                                    <input name="idColaboradorCyber" type="checkbox" id="idColaboradorCyber" data-dojo-type="dijit/form/CheckBox" />
                                </td>
                                <td colspan="2" style="width: 57%;"></td>
                            </tr>
                            <tr>
                                <td class="tdRight" style="width: 23%;">
                                    <label for="idColaboradorCyber">Assinatura Certificado:</label>
                                </td>
                                <td style="width: 77%;" colspan="3">
                                    <table class="Table">
                                        <tr>
                                            <td class="dojoTextBoxIECorrect" style="width:95%;">
                                                 <input id="nomAssinaturaCertificado" name="nomAssinaturaCertificado" data-dojo-type="dijit/form/ValidationTextBox" style="width: 100%;" disabled="disabled" />
                                                 <div data-dojo-type="dijit/Tooltip" data-dojo-props="connectId:'nomAssinaturaCertificado',position:['above']">
                                                     O tamanho máximo da imagem é de <b>400 KB</b>. As extensões suportadas pelo sistema são: <b>'.jpg', '.jpeg', '.gif', '.png', '.bmp'</b> .
                                                 </div>
                                            </td>
                                             <td colspan="2" style="text-align: right;width: 05%;">
                                                 <input accept="image/jpg" 
                                                        class="browseButton"
                                                        name="uploadedfile"
                                                        type="file" 
                                                        force:"iframe"  
                                                        data-dojo-props='iconClass:"dijitEditorIcon dijitEditorIconUpload" '
                                                        isDebug:"true"
                                                        data-dojo-type="dojox/form/Uploader" 
                                                        id="uploaderAssinatura" />
                                                    @* <input id="btnUp" style="min-width: 18px;" />*@
                                            </td>
                                        </tr>
                                    </table>
                                    

                                </td>
                            </tr>
                        </table>
                    </form>
                    <div data-dojo-type="dijit/TitlePane" id="tgComissao" data-dojo-props="title: 'Comissão'" open="false" style="display:none;">
                        <div class="linha_acoes" style="width: 100%"></div>
                        <table style="width: 100%" class="Alinhamento">
                            <tr style="width: 100%">
                                <td style="text-align: left; width: 10%">
                                    <div class="linkAcoes" id="linkAcoesComiss"></div>
                                </td>
                                <td class="Alinhamento">
                                    <div id="btnNovaComiss"></div>
                                </td>
                            </tr>
                        </table>
                        <div class="linha_acoes" style="width: 100%"></div>
                        <div data-dojo-type="dijit/layout/ContentPane" style="max-width: 664px; width: 100%; height: 30px; padding-left: 0px; padding-bottom: 0px;">
                            <div id="gridComissaoHeader" style="height: 27px; width: 602px;"></div>
                        </div>
                        <div class="gridCadBasicPartial" style="margin-top: -4px">
                            <div id="gridComissao"></div>
                        </div>
                    </div>
                    <div data-dojo-type="dijit/TitlePane" id="TagProfessor" data-dojo-props="title: 'Professor'" open="true">
                        <div>
                            <form id="formProfessor" data-dojo-type="dojox/form/Manager">
                                <table class="Table">
                                    <tr>
                                        <td class="CorpoFormAux AlinhaDireita" style="width: 20%;">
                                            <label for="formaPagto">Forma Pagamento:</label></td>
                                        <td class="dojoTextBoxIECorrect" style="width: 30%;">
                                            <input id="formaPagto" name="formaPagto" class="alignDojoData" style="width: 100%;" />
                                        </td>
                                        <td class="CorpoFormAux AlinhaDireita" style="width: 20%;">
                                            <label for="ckContrato">Contratado:</label></td>
                                        <td style="width: 30%;">
                                            <table class="Table">
                                                <tr>
                                                    <td style="width: 70%;">
                                                        <input name="idComissao" type="checkbox" id="ckContrato" data-dojo-type="dijit/form/CheckBox" checked="true" />
                                                    </td>
                                                    <td class="CorpoFormAux AlinhaDireita" style="width: 20%;">
                                                        <label for="ckCoordenador">Coordenador:</label>
                                                    </td>
                                                    <td style="width: 10%;">
                                                        <input name="ckCoordenador" type="checkbox" id="ckCoordenador" data-dojo-type="dijit/form/CheckBox" onclick="toglleCordenador(this.checked);" />
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td colspan='3' class="CorpoFormAux AlinhaDireita" style="width: 20%;">
                                            <label >Produto Assinatura Certificado:</label>
                                        </td>
                                        <td>
                                            <table class="Table">
                                                <tr>
                                                    <td class="alignDojoData">
                                                        <select
                                                            id="cbProdutoAssinatura"
                                                            data-dojo-id="cbProdutoAssinatura"
                                                            data-dojo-props='dropDown:true'
                                                            name="cbProdutoAssinatura"
                                                            multiple="true"
                                                            onAfterAddOptionItem="setoptions"
                                                            data-dojo-type="dojox/form/CheckedMultiSelect" style="width: 97%;" required="required">
                                                        </select>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="CorpoFormAux AlinhaDireita">
                                            <label for="valInterno">Valor Interna:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">
                                            @*<input id="valInterno"  maxlength="8"  style="width: 100%" value="0" data-dojo-type="dijit/form/TextBox" />*@
                                            <input value="0" style="width: 100%;" id="valInterno" name="valInterno" maxlength="8"
                                                data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
                                                onkeypress="mascaraFloat(document.getElementById('valInterno'));" data-dojo-type="dijit/form/NumberTextBox" />
                                        </td>
                                        <td class="CorpoFormAux AlinhaDireita">
                                            <label for="valExterno">Valor Externa:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">
                                            @*<input id="valExterno" maxlength="8"  style="width: 99%" value="0" data-dojo-type="dijit/form/TextBox" />*@
                                            <input value="0" style="width: 99%;" id="valExterno" name="valExterno" maxlength="8"
                                                data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
                                                onkeypress="mascaraFloat(document.getElementById('valExterno'));" data-dojo-type="dijit/form/NumberTextBox" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="CorpoFormAux AlinhaDireita">
                                            <label for="dcChapa">Matrícula Número:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">
                                            <input id="dcChapa" maxlength="12" style="width: 100%" data-dojo-type="dijit/form/TextBox" />
                                        </td>
                                        <td class="CorpoFormAux AlinhaDireita nowrap">
                                            <label for="pcTerminoEstagio">% Término de Estágio:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">                                           
                                            <input id="pcTerminoEstagio" type="text" data-dojo-type="dijit/form/NumberSpinner"
                                                name="pcTerminoEstagio"
                                                style="width: 95%" onkeypress="mascaraInt(document.getElementById('pcTerminoEstagio'));"
                                                data-dojo-props="smallDelta:1, constraints:{min:0,max:100,places:0,pattern:'###'}, style: 'width:50px'" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="CorpoFormAux AlinhaDireita nowrap">
                                            <label for="vlrTerminoEstagio">Valor Término de Estágio:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">
                                            <input value="0" id="vlrTerminoEstagio" style="width: 100%" maxlength="8"
                                                data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
                                                onkeypress="mascaraFloat(document.getElementById('vlrTerminoEstagio'));" data-dojo-type="dijit/form/NumberTextBox" />
                                        </td>
                                        <td class="CorpoFormAux AlinhaDireita">
                                            <label for="vlrRematricula">Valor Rematrícula:</label>
                                        </td>
                                        <td class="dojoTextBoxIECorrect">
                                            <input value="0" id="vlrRematricula" style="width: 100%" maxlength="8"
                                                data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
