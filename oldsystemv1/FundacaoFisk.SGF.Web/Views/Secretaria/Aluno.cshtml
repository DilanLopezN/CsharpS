<div id="cadAlunoRestricao" data-dojo-type="dijit.Dialog" title="Cadastro de Restrição Financeira" class="Dialogo">



    <div class="corpo_interno">
        <form data-dojo-type="dojox/form/Manager" enctype="multipart/form-data" id="formAlunoRestricao" data-dojo-id="formAlunoRestricao" class="Formulario">
            <div class="apresentadorMsg" id="apresentadorMensagemAlunoRestricao" style="display:none;"></div>
            <input id="cd_aluno_resticao" name="cd_aluno_resticao" type="hidden" value="0" />
            <input id="id_grid_aluno_restricao" name="id_grid_aluno_restricao" type="hidden" value="0" />
            <div class="CorpoForm">
                <table class="Table">
                    <tr>

                        <td class="CorpoFormAux AlinhaDireita" style="width: 10%;">
                            <label>Orgão:</label>
                        </td>
                        <td class="dojoTextBoxIECorrect" style="width: 40%;">
                            <select id="cbOrgaoFinanceiro" name="cbOrgaoFinanceiro" required="true" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;"></select>
                        </td>

                        <td class="CorpoFormAux AlinhaDireita" style="width: 10%;">
                            <label>Responsável:</label>
                        </td>
                        <td class="dojoTextBoxIECorrect" style="width: 40%;">
                            <input type="hidden" id="cd_atendente_usuario_restricao" />
                            <input type="text" style="width: 100%;" data-dojo-type="dijit.form.TextBox" id="no_atendente_usuario_restricao" disabled="disabled" />
                        </td>
                    </tr>
                    <tr>
                        <td colspan="1" class="CorpoFormAux AlinhaDireita" style="width: 10%">
                            <label>Inclusão:</label>
                        </td>
                        <td class="dojoTextBoxIECorrect" style="width: 40%">
                            <input type="text"
                                   style="width: 100%;"
                                   id="dt_inicio_restricao"
                                   data-dojo-type="dijit.form.DateTextBox"
                                   onkeypress="mascaraDataHora(event, this)" maxlength="10" />
                        </td>
                        <td colspan="1" class="CorpoFormAux AlinhaDireita" style="width: 10%">
                            <label>Saída:</label>
                        </td>
                        <td class="dojoTextBoxIECorrect" style="width: 40%">
                            <input type="text"
                                   style="width: 100%;"
                                   id="dt_final_restricao"
                                   data-dojo-type="dijit.form.DateTextBox"
                                   onkeypress="mascaraDataHora(event, this)" maxlength="10" />
                        </td>
                    </tr>
                </table>

            </div>
            <div class="CaixaAcao">
                <div class="linha_acoes" style="width: 100%"></div>
                <div class="Alinhamento">
                    <span id="divAdicionarAlunoRestricao">
                        <input id="adicionarAlunoRestricao" title="Incluir" />
                    </span>
                    <span id="divAlterarAlunoRestricao">
                        <input id="alterarAlunoRestricao" title="Alterar" />
                    </span>
                    <span id="divExcluirAlunoRestricao">
                        <input id="deleteAlunoRestricao" title="Apagar" type="button" />
                    </span>
                    <span id="divLimparAlunoRestricao">
                        <input id="limparAlunoRestricao" title="Limpar" />
                    </span>
                    <span id="divCancelarAlunoRestricao">
                        <input type="button" id="cancelarAlunoRestricao" title="Cancelar" />
                    </span>
                    <span id="divFecharAlunoRestricao">
                        <input id="fecharAlunoRestricao" title="Fechar" type="button" />
                    </span>
                </div>
                <div class="linha_acoes" style="width: 100%"></div>
            </div>
        </form>
    </div>
</div>
@*fim*@

