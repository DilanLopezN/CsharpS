@section scriptsBody{
    <script type="text/javascript">
        document.getElementById("labelTitulos").innerHTML = "Aluno/Cliente";
        //**** Requerires para a Tabulação ****\\
        require([
                "dojo/NodeList-traverse",
                "dijit/form/TextBox",
                "dijit/form/Select",
                "dojox/validate/web",
                "dojox/validate/check",
                "dijit/form/NumberTextBox",
                "dijit/form/ValidationTextBox",
                "dijit/form/Form",
                "dojox/form/Manager",
                "dijit/Tooltip",
                "dijit/form/TimeTextBox",
                "dojox/charting/Chart"
        ]);
        inicialAluno();
        montaGridAluno('@Permissoes');
        montarElementoEmail();
    </script>

    <style>
        /*Mudar as cores dos horários alocados diferente de disponivies e alocados*/
        /*Css abaixo aplicado para sala*/
        .claro .dojoxCalendarEvent.Calendar5 .bg {
            background-color: #e6ad00;
        }

        .claro .dojoxCalendarEvent.Calendar5.Hovered .bg {
            background-color: #ffc610;
        }

        .claro .dojoxCalendarEvent.Calendar5.Selected .bg {
            background-color: #b38600;
        }

        .claro .dojoxCalendarEvent.Calendar5 .startTime {
            background-color: #b38600;
        }

        .claro .dojoxCalendarEvent.Calendar5.Hovered .startTime {
            background-color: #e6ad00;
        }

        .claro .dojoxCalendarEvent.Calendar5.Selected .startTime {
            background-color: #9a7300;
        }

        .claro .dojoxCalendarEvent.Calendar5 .handle {
            background-color: #e6ad00;
        }
    </style>
}

