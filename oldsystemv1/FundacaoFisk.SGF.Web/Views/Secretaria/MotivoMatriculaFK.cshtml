@Scripts.Render("~/bundles/motivoMatricula")
<script type="text/javascript">
    montarMtvMat();
</script>

<div class="corpo_interno">
    <div class="EspacoCaixa caixaPesquisa">
        <table class="Table">
            <tr>
                <td class="Pesquisa">
                    <label for="pesquisaMtvMatFK">Descrição: </label>
                    <input id="pesquisaMtvMatFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox" class="campoGrande" style="width: 54%">
                    <label class="AlinhaDireita;" for="inicioDescMtvMatFK">Início:</label>
                    <input type="checkbox" id="inicioDescMtvMatFK" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
                    <span class="nowrap">
                        <label for="statusMtvMatFK" class="AlinhaDireita;">Status:</label>
                        <input id="statusMtvMatFK" class="margemCampoEsq">
                    </span>
                </td>
                <td>
                    <input id="pesquisarMtvMatFK" title="Pesquisar" />
                </td>
            </tr>
        </table>
    </div>

    <div style="padding-top: 5px;width: 100%">
        <div class="linha_acoes" style="width: 100%"></div>
        <table style="width: 100%">
            <tr style="width: 100%">
                <td style="text-align: center; width: 100%;">
                    <input type="button" id="selecionaMotivoMatricluaFK" />&nbsp;
                    <input type="button" id="fecharMotivoMatricluaFK" title="Fechar" />
                </td>
            </tr>
        </table>
        <div class="linha_acoes" style="width: 100%"></div>
    </div>

    <div style="width:100%; height:430px;margin-bottom:5px;">
        <div data-dojo-type="dijit/layout/ContentPane" id="panelMotivoMatricluaFK" style="width: 100%; height: 430px; padding-top: 5px; padding-left: 0px;padding-bottom:5px;">
            <div id="gridMotivoMatricluaFK"></div>
        </div>
    </div>

</div>
<input id="ehSelectGradeMMatriculaFK" type="hidden" value="false"/>