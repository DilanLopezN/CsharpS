            <span id="divExcluirMensagem" style="display: none;">
                <input type="button" id="deleteMensagem" title="Excluir" /></span>
            <span id="divClearMensagem">
                <input type="button" id="limparMensagem" title="Limpar" /></span>
            <span id="divCancelarMensagem" style="display: none;">
