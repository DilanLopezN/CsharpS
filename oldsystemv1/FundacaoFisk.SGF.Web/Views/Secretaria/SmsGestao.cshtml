    <div class="corpo_interno" style="max-width: 98%; height: 57%; max-height: 57%">
        <div >
            <div class="CaixaAcao">
                <div class="linha_acoes" style="width: 100%"></div>
                <div class="Alinhamento">
                                                
                    <select id="tipoSmsCompor" name="tipoSmsCompor" data-dojo-type="dijit/form/FilteringSelect" style="width: 100%;" required="true"></select>
                    <br /><br />
                    <span class="linkSelecionados" id="linkCamposMesclagem"></span>
                </div>
                                                
                <div class="linha_acoes" style="width: 100%"></div>
            </div>
                                            
        </div>
        <table class="Table">
            <tr>
                <h3>Escreva a Mensagem de SMS:</h3>
                <br />
