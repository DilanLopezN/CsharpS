@*Form de cadastro dos parametros*@
<div id="cadParametroSMS" data-dojo-type="dijit/Dialog" class="Dialogo"  style="max-width: 750px;"  title="Parametrização do Serviço de SMS">
        
    <div class="sessao_form" style="margin-left: -10px;">
        <span id="counter_form">Sua sess&atilde;o expira em 59 min 59 seg</span>
    </div>

    <div class="corpo_interno" style="max-width: 720px;">
        <div class="apresentadorMsg" id="apresentadorMensagemTurma" style="display: none;"></div>
        <div data-dojo-type="dijit/layout/TabContainer" id="tabContainerTurma" class="Abas" onclick="selecionaTab" style="min-height: 240px; max-height: 300px; height: 100%;">
            <div data-dojo-type="dijit/layout/ContentPane" id="tabPrincipalTurma" title="Principal" style="height: 100%;">
                <form name="formTurmaPrincipal" id="formTurmaPrincipal" data-dojo-type="dojox/form/Manager" style="width: 100%">
                    <table class="Table">
                        <tr>
                            <td class="CorpoFormAux AlinhaDireita" style="width: 30%;">
                                <label>Seu Código de Usuário (NumUsu):</label></td>
                            <td class="dojoTextBoxIECorrect" colspan="3" style="width: 30%;">
                                <input id="numUsu" name="numUsu" type="text" data-dojo-type="dijit.form.ValidationTextBox" style="width: 100%" required="true" />
                            </td>
                        </tr>
                        <tr>
                            <td class="CorpoFormAux AlinhaDireita">
                                <label>Celular da Unidade:</label>
                            </td>
                            <td class="dojoTextBoxIECorrect">
                                <input type="text" style="width: 100%;" id="seuNum" name="seuNum" type="text" data-dojo-type="dijit.form.ValidationTextBox" required="required" />
                            </td>

                        </tr>
                        <tr>
                            <td class="CorpoFormAux AlinhaDireita" style="width: 30%;">
                                <label>Senha do Serviço:</label></td>
                            <td class="dojoTextBoxIECorrect" colspan="3" style="width: 70%;">
                                <input type="password" id="senha" name="senha" type="text" data-dojo-type="dijit.form.ValidationTextBox" style="width: 100%" required="true"/>
                            </td>
                        </tr>
                        <tr>
                            <td class="CorpoFormAux AlinhaDireita" style="width: 30%;">
                                <label>Confirme a Senha do Serviço:</label></td>
