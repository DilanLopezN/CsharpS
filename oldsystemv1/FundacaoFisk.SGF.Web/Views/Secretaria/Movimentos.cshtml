                    <td style="width: 30%" class="dojoTextBoxIECorrect">
                        <input id="dtaEmisTit"
                               style="width: 100%" name="dtaEmisTit" size="61" required="required"
                               data-dojo-type="dijit.form.DateTextBox"
                               onkeypress="mascaraDataHora(event, this)" maxlength="10" />
                    </td>
                </tr>
                <tr>
                    <td class="CorpoFormAux AlinhaDireita nowrap" style="width: 20%">
                        <label for="dtaVencTit">Vencimento:</label>
                    </td>
                    <td style="width: 30%" class="dojoTextBoxIECorrect">
                        <input id="dtaVencTit"
                               name="dtaVencTit" size="61"
                               style="width: 100%" required="required"
                               data-dojo-type="dijit.form.DateTextBox"
                               onkeypress="mascaraDataHora(event, this)" maxlength="10" />
                    </td>
                    <td class="CorpoFormAux AlinhaDireita nowrap" style="width: 20%">
                        <label for="valorTit">Valor do Título:</label>
                    </td>
                    <td style="width: 30%" class="dojoTextBoxIECorrect">
                        <input id="valorTit" name="valorTit" required="required"
                               data-dojo-type="dijit/form/NumberTextBox"
                               maxlength="10" style="width: 100%"
                               data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
                               onkeypress="mascaraFloat(document.getElementById('valorTit'));" />
                    </td>
                </tr>
                <tr>
                    <td class="tdRight nowrap" style="width: 20%">
                        Local de Movimento:
                    </td>
                    <td colspan="3" style="width: 80%;" class="dojoTextBoxIECorrect nowrap">
                        <input id="cd_bancoTit" disabled="disabled" type="hidden" hidden />
                        <select id="bancoTit" name="bancoTit" required="required" style="width: 100%;" data-dojo-type="dijit/form/FilteringSelect" />
                    </td>
                </tr>
            </table>
            <div data-dojo-type="dijit/TitlePane" data-dojo-props="title: 'Valores Taxa de Cartão'" id="tgCartao" open="true" style="display: none">
                <table>
                    <tr>
                        <td class="tdLeft" style="width: 5%;">
                            <label>Taxa(%):</label>
                        </td>
                        <td style="width: 20%;" class="dojoTextBoxIECorrect nowrap">
                            <input type="text" id="pc_taxa_cartao" name="pc_taxa_cartao" required="false" style="width: 100%;" data-dojo-type="dijit/form/NumberTextBox" />
                        </td>

                        <td class="tdLeft" style="width: 8%">
                            <label>D+(dias):</label>
                        </td>
                        <td style="width: 20%;" class="dojoTextBoxIECorrect">
                            <input type="text" id="nm_dias_cartao" name="nm_dias_cartao" required="false" data-dojo-type="dijit/form/NumberTextBox" style="width: 100%;" />
                        </td>
                        <td class="tdLeft" style="width: 10%">
                            <label>Valor Taxa:</label>
                        </td>
                        <td style="width: 20%;" class="dojoTextBoxIECorrect">
                            <input id="vl_taxa_cartao" name="vl_taxa_cartao" required="false"
                                   data-dojo-type="dijit/form/NumberTextBox"
                                   maxlength="10" style="width: 100%"
                                   data-dojo-props="smallDelta:1, constraints:{min:0, pattern:'##.00'}"
                                   onkeypress="mascaraFloat(document.getElementById('vl_taxa_cartao'));" />
                        </td>
                    </tr>
                </table>
            </div>
        </form>
        <div class="containerCrud" style="width: 100%; padding-top: 20px;">
            <div class="linha_acoes" style="width: 100%"></div>
            <div style="text-align: center">
                <span id="divIncluirTitulo">
                    <input type="button" id="incluirTitulo" title="incluir" /></span>
                <span id="divAlterarTitulo" style="display: none;">
                    <input type="button" id="alterarTitulo" title="Alterar" /></span>
                <span id="divExcluirTitulo" style="display: none;" />
                <span id="divClearTitulo">
                    <input type="button" id="limparTitulo" title="Limpar" /></span>
                <span id="divCancelarTitulo" style="display: none;">
                    <input type="button" id="cancelarTitulo" title="Cancelar" /></span>
                <span id="divFecharTitulo">
                    <input type="button" id="fecharTitulo" title="Fechar" /></span>
            </div>
            <div class="linha_acoes" style="width: 100%"></div>
        </div>
    </div>
</div>

