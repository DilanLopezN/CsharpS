@Scripts.Render("~/bundles/motivoNaoMatricula")
<script type="text/javascript">
    montarMtvNMat();
</script>

<div class="corpo_interno">
    <div class="EspacoCaixa caixaPesquisa">
        <table class="Table">
            <tr>
                <td class="Pesquisa">
                    <label for="pesquisaMtvNMat">Descrição: </label>
                    <input id="pesquisaMtvNMatFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox" class="campoGrande" style="width: 52%">
                    <label class="AlinhaDireita;" for="inicioDescMtvNMatFK">Início:</label>
                    <input type="checkbox" id="inicioDescMtvNMatFK" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
                    <span class="nowrap">
                        <label for="statusMtvNMatFK" class="AlinhaDireita;">Status:</label>
                        <input id="statusMtvNMatFK" class="margemCampoEsq">
                    </span>
                </td>
                <td>
                    <input id="pesquisarMtvNMatFK" title="Pesquisar" />
                </td>
            </tr>
        </table>
    </div>

    <div style="padding-top: 5px; width: 100%">
        <div class="linha_acoes" style="width: 100%"></div>
        <table style="width: 100%">
            <tr style="width: 100%">
                <td style="text-align: center; width: 100%;">
                    <input type="button" id="selecionaMotivoNMatricluaFK" />&nbsp;
                    <input type="button" id="fecharMotivoNMatricluaFK" title="Fechar" />&nbsp;
                </td>
            </tr>
        </table>
        <div class="linha_acoes" style="width: 100%"></div>
    </div>

    <div style="width: 100%; height: 430px;padding-bottom:5px;">
        <div data-dojo-type="dijit/layout/ContentPane" id="panelMotivoNMatricluaFK" style="width: 100%; height: 430px; padding-top: 5px; padding-left: 0px">
            <div id="gridMotivoNMatricluaFK"></div>
        </div>
    </div>
</div>
<input id="ehSelectGradeMNMatriculaFK" type="hidden" value="false"/>