
@Scripts.Render("~/bundles/tipoNFFk")
<div class="corpo_interno" style="height: 90%">
    <div class="apresentadorMsg" id="apresentadorMensagemTipoNFFK" style="display: none;"></div>
    <div class="EspacoCaixa caixaPesquisa">
        <table class="Table">
            <tr>
                <td class="Pesquisa tdRight nowrap" style="width: 5%">
                    <label for="descTpNFFK" class="AlinhaDireita">Descrição :</label>
                </td>
                <td class="dojoTextBoxIECorrect" style="width: 40%;">
                    <input id="descTpNFFK" name="descTpNFFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox" class="campoGrande" style="width: 100%">
                </td>
                <td class="tdRight" style="width: 5%">
                    <label class="AlinhaDireita;" for="inicioTpNFFK">Início:</label>
                </td>
                <td style="width: 5%" class="dojoTextBoxIECorrect">
                    <input type="checkbox" id="inicioTpNFFK" name="inicioTpNFFK" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
                </td>
                <td rowspan="2" style="padding-left: 10px; width: 10%">
                    <input data-dojo-type="dijit/form/Button" data-dojo-props="iconClass:'dijitEditorIconSearchSGF'" type="button" id="pesquisarTipoNFFK" />
                </td>
            </tr>
            <tr>
                <td class="tdRight" style="width: 10%;">
                    <label for="cbMovtoFK" class="AlinhaDireita;">Natureza:</label>
                </td>
                <td class="dojoTextBoxIECorrect" style="width: 40%;">
                    <input data-dojo-type="dijit/form/FilteringSelect" id="cbMovtoFK" name="cbMovtoFK" style="width: 50%;" required="false" />
                </td>
                <td class="tdRight" style="width: 10%;">
                    <label for="statusTpNFFK" class="AlinhaDireita;">Status:</label>
                </td>
                <td style="width: 7%;" class="dojoTextBoxIECorrect">
                    <input id="statusTpNFFK" name="statusTpNFFK" class="margemCampoEsq">
                </td>
            </tr>
        </table>
    </div>
    <div style="padding-top: 5px; width: 100%">
        <div class="linha_acoes" style="width: 100%"></div>
        <table style="width: 100%">
            <tr style="width: 100%">
                <td style="text-align: center; width: 100%;">
                    <input type="button" id="selecionaTipoNFFK" />&nbsp;
                    <input type="button" id="fecharTipoNFFK" title="Fechar" />
                </td>
            </tr>
        </table>
        <div class="linha_acoes" style="width: 100%"></div>
    </div>

    <div data-dojo-type="dijit/layout/ContentPane" id="panelTipoNFFK" style="width: 100%; height: 410px; padding-top: 5px; padding-left: 0px">
        <div id="gridTipoNFFK" style="height: 400px"></div>
    </div>
</div>
