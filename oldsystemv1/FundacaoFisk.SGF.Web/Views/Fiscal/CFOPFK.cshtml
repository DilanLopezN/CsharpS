
@Scripts.Render("~/bundles/CFOPFK")
<div class="corpo_interno">
    <div class="apresentadorMsg" id="apresentadorMensagemCFOPFK" style="display: none;"></div>
    <div class="EspacoCaixa caixaPesquisa">
        <table class="Table">
            <tr>
                <td class="tdRight" style="width: 10%">
                    <label for="descCFOPFK" class="AlinhaDireita">Descrição :</label>
                </td>
                <td class="dojoTextBoxIECorrect" style="width: 40%;">
                    <input id="descCFOPFK" name="descCFOPFK" maxlength="128" type="text" data-dojo-type="dijit/form/TextBox"  style="width: 100%">
                </td>
                <td class="tdRight" style="width: 7%">
                    <label class="AlinhaDireita;" for="inicioCFOPFK">Início:</label>
                </td>
                <td style="width: 4%" class="dojoTextBoxIECorrect">
                    <input type="checkbox" id="inicioCFOPFK" name="inicioCFOPFK" data-dojo-type="dijit/form/CheckBox" class="margemCampoEsq" />
                </td>
                <td class="tdRight" style="width: 14%;">
                    <label for="nm_CFOPFK" class="AlinhaDireita;">Número CFOP:</label>
                </td>
                <td style="width: 15%;" class="dojoTextBoxIECorrect">
                    <input id="nm_CFOPFK" name="nm_CFOPFK"  type="text" style="width: 100%" data-dojo-type="dijit/form/TextBox">
                </td>
                <td rowspan="1"style="width: 10%">
                    <input data-dojo-type="dijit/form/Button" data-dojo-props="iconClass:'dijitEditorIconSearchSGF'" type="button" id="pesquisarCFOPFK" />
                </td>
            </tr>
        </table>
    </div>
    <div style="padding-top: 5px; width: 100%">
        <div class="linha_acoes" style="width: 100%"></div>
        <table style="width: 100%">
            <tr style="width: 100%">
                <td style="text-align: center; width: 100%;">
                    <input type="button" id="selecionaCFOPFK" />&nbsp;
                    <input type="button" id="fecharCFOPFK" title="Fechar" />
                </td>
            </tr>
        </table>
        <div class="linha_acoes" style="width: 100%"></div>
    </div>

    <div data-dojo-type="dijit/layout/ContentPane" id="panelCFOPFK" style="width: 100%; height: 410px; padding-top: 5px; padding-left: 0px">
        <div id="gridCFOPFK" style="height: 400px"></div>
    </div>
</div>